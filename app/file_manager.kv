<FileManager>:
    orientation: 'vertical'
    Label:
        text: 'Simple File Manager'
    Button:
        text: 'Browse Files'
        on_press: root.browse_files()
    Button:
        text: 'Storage Analysis'
        on_press: root.storage_analysis()
    Button:
        text: 'Security Settings'
        on_press: root.security_settings()
        